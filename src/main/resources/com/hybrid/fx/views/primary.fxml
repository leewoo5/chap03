<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<AnchorPane id="AnchorPane" fx:id="primary" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hybrid.fx.controllers.PrimaryController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="40.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <BorderPane prefHeight="75.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
               <top>
                  <HBox prefHeight="72.0" prefWidth="600.0" BorderPane.alignment="CENTER">
                     <children>
                        <TextField fx:id="url" maxWidth="1.7976931348623157E308" prefHeight="23.0" prefWidth="609.0" />
                     </children>
                  </HBox>
               </top>
               <center>
                  <WebView fx:id="view" prefHeight="300.0" prefWidth="600.0" BorderPane.alignment="CENTER" />
               </center>
               <bottom>
                  <Button fx:id="home" mnemonicParsing="false" text="Home" BorderPane.alignment="CENTER" />
               </bottom>
            </BorderPane>
            <Label fx:id="label" text="Welcome!" />
            <Button fx:id="button" mnemonicParsing="false" text="%button.text" />
         </children>
      </VBox>
   </children>
</AnchorPane>
